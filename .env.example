# EdgeDB Configuration
EDGEDB_USER=edgedb
EDGEDB_PASSWORD=<your_strong_password>  # Replace with a strong password
EDGEDB_DATABASE=edgedb
EDGEDB_HOST=edgedb
EDGEDB_PORT=5656
EDGEDB_TLS_SECURITY=strict

# Connection string for EdgeDB
EDGEDB_DSN=edgedb://${EDGEDB_USER}:${EDGEDB_PASSWORD}@${EDGEDB_HOST}:${EDGEDB_PORT}/${EDGEDB_DATABASE}?tls_security=${EDGEDB_TLS_SECURITY}

# Next.js Application Configuration
NEXTAUTH_SECRET=<your_nextauth_secret>  # Replace with a secure random string
NEXTAUTH_URL=https://yourdomain.com  # Replace with your application's URL
